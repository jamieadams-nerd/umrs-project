{
  "labels": {
    "GENERAL": {
      "name": "General",
      "level": "s0",
      "description": "The broad general unclassified level suitable for anything openly unclassified, routine, not requiring special controls.",
      "handling": "No special handling requirements. Standard system access controls apply."
    },
    "U-PUBLIC": {
      "name": "Unclassified Public",
      "level": "s1",
      "description": "Intended for material that is still unclassified but suitable for public dissemination, documentation, public website assets, etc."
    },
    "U-INTERNAL": {
      "name": "Unclassified Internal",
      "level": "s2",
      "description": "Organizational level unclassified information. Internal-use unclassified information that is still above the purely  sensitive level. This covers items where integrity matters more strongly (engineering docs, config examples, internal processes)."
    },
    "CUI": {
      "name": "Controlled Unclassified Information",
      "level": "s3",
      "description": "Information requiring safeguarding or dissemination controls under U.S. law, regulation, or government policy. This is the base CUI level to which specific categories may be attached.",
      "handling": "Mark clearly as CUI. Enforce access restrictions. Use approved storage, transport, and encryption. Ensure auditing and accountability."
    }
  },
